{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node",
      "commands": [
        "yarn prisma generate", 
        "yarn build"  
      ]
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods":[
          "GET",
          "POST",
          "PUT",
          "DELETE",
          "OPTIONS"
      ]
    }

  ]
}
