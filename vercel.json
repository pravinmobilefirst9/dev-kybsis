{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node",
        "postinstall": "prisma generate"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods":[
            "GET",
            "POST",
            "PUT",
            "DELETE",
            "OPTIONS"
        ]
      }
  
    ] 
  }
  